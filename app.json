{
  "expo": {
    "name": "DY Patil PGDM Institute SeQR Scan",
    "slug": "DYPatilPGDMInstitute_SeQR_Scan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logos/DYPATIL_INSTITUTE.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.dy-patil-institute-akurdi-pune",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logos/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.dy_patil_institute_akurdi_pune",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "keystore": {
        "keystorePath": "credentials/android/keystore.jks",
        "keystorePassword": "123456",
        "keyAlias": "my-key-alias"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logos/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "cameraUsageDescription": "This app requires camera access to scan QR codes",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          },
          "ios": {}
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21110350-ed32-4aef-9182-cbcae8ce0c73"
      }
    }
  }
}