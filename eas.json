{
  "cli": {
    "version": ">= 16.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.18.0",
      "env": {
        "EXPO_USE_YARN": "true"
      },
      "hooks": {
        "preInstall": "corepack enable && corepack prepare yarn@3.6.4 --activate"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "18.18.0",
      "env": {
        "EXPO_USE_YARN": "true"
      },
      "hooks": {
        "preInstall": "corepack enable && corepack prepare yarn@3.6.4 --activate"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "18.18.0",
      "env": {
        "EXPO_USE_YARN": "true"
      },
      "hooks": {
        "preInstall": "corepack enable && corepack prepare yarn@3.6.4 --activate"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
